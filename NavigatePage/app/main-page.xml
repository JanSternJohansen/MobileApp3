<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo">
    <ActionBar title="Navigate Pages" icon=""></ActionBar>
    <GridLayout columns="*, *" rows="2*, *, * , *, *" style="margin:10px">
        <Label text="Main Page 1" row="0" col="0" colSpan="2" class="h1 text-center"/>
        <Label text="From Second:" row="1" col="0" class="h2 text-left" textWrap="true"/>
        <Label text="Her.." id="txtFromSecond" row="1" col="1" class="h2 text-left" textWrap="true"/>
         <Label text="To Second:" row="2" col="0" class="h2 text-left" textWrap="true"/>
         <TextField id="txtToSecond" hint="Skriv her til Second" row="2" col="1" class="h2 text-left"/>

     <Button text="Go to Second" tap="onGoSecond" row="3" col="0" colSpan="2"  class="-primary"/>
    </GridLayout>
</Page>
